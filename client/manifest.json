{
    "manifest_version": 3,
    "name": "HeadlineAI",
    "version": "1.0",
    "description": "A simple Chrome extension",
    "action": {
        "default_popup": "popup/popup.html",
        "default_icon": "images/icon16.png"
    },
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://www.foxnews.com/media/*"
            ],
            "js": [
                "scripts/content.js"
            ]
        }
    ],
    "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    }
}